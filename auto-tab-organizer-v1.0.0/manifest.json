{
  "manifest_version": 3,
  "name": "Auto Tab Organizer (LLM)",
  "version": "0.1.0",
  "description": "Organize tabs into logical groups using an LLM.",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Organize Tabs",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}


